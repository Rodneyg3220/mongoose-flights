<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Rating<br>By<br>Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% movies.forEach(function(m) { %>
      <tr>
        <td><%= m.airline %></td>
        <td><%= m.releaseYear %></td>
        <td><a href="/flights/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
